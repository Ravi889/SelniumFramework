<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="SelniumFramework" packages="">
<Appenders>
    <File name="MyFile" fileName="E:\SelniumFramework\logs\app2">
        <PatternLayout>
            <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
        </PatternLayout>
    </File>
</Appenders>
<Loggers>
    <Root level="error">
        <AppenderRef ref="MyFile"/>
    </Root>
</Loggers>
</Configuration>